                            { header: "Web designers", icon: "../assets/icons/ic-instagram-black.svg", title: "Fresh content", desc: "Every story you post on Instagram, it is automatically saved in your account. Auto updates you unique content." },
                        ],
                            templates: [
                                { title: "Popover", desc: "Showcases on every page that gives the feel of a living & breathing website.", src: "../assets/images/slider-template-1.png" },
                                { title: "Highlights", desc: "Mimics the same functionality of the Instagram Highlights on your website.", src: "../assets/images/slider-template-2.png" },
                                { title: "Slider", desc: "Display your stories in a full slideshow layout that catches your visitorsâ€™ attention.", src: "../assets/images/slider-template-3.png" },
                                { title: "Gallery", desc: "One widget with multiple stories and embed them as a full story gallery on your website.", src: "../assets/images/slider-template-4.png" },

                            ],
                                featureLabel: [
                                    { text: "Stories from specific date", icon: "../assets/icons/ic-schedule.svg" },
                                    { text: "Customize color", icon: "../assets/icons/ic-fill-black.svg" },
                                    { text: "Follow button", icon: "../assets/icons/ic-instagram-black.svg" },
                                    { text: "Multiple layouts", icon: "../assets/icons/ic-frame-black.svg" },
                                    { text: "Advance moderation", icon: "../assets/icons/ic-setting-black.svg" },
                                    { text: "Custom CSS", icon: "../assets/icons/ic-tag-black.svg" },
                                ],
                                    featureBenefitsList: [
                                        { text: "Simple instalation. No developers needed", src: "../assets/icons/ic-checkmark.svg" },
                                        { text: "Embed on different CMS platforms", src: "../assets/icons/ic-checkmark.svg" },
                                        { text: "Auto sync new stories", src: "../assets/icons/ic-checkmark.svg" },
                                    ],
                                        featurePreviewsList: [
                                            { text: "HTML", src: "../assets/icons/ic-programing-blue.svg", activateClass: "first-feature-preview__item--activate" },
