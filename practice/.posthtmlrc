{
    plugins: {
        "posthtml-include": {
            "root": "./src"
        },
        "posthtml-doctype": { "doctype": "HTML 5" },
        "posthtml-expressions": {
            locals: {
                frameSelections: [
                    { text: "Highlights", src: "../assets/icons/ic-frame-1.svg", activateClass: "" },
                    { text: "Popover", src: "../assets/icons/ic-frame-2.svg", activateClass: "" },
                    { text: "Inline slideshows", src: "../assets/icons/ic-frame-3.svg", activateClass: "" },
                    { text: "Story list gallery", src: "../assets/icons/ic-frame-4.svg", activateClass: "" },
                    { text: "Carousel gallery", src: "../assets/icons/ic-frame-5.svg", activateClass: "frame-selection__button--activate" },
                ],
                    socialBenefits: [
                        { title: "Auto-Sync", desc: "Instagram Stories", src: "../assets/images/logo-instagram.png" },
                        { title: "Upload", desc: "Facebook Stories", src: "../assets/images/logo-facebook.png" },
                        { title: "Sync", desc: "Tiktok Stories", src: "../assets/images/logo-tiktok.png" },
                        { title: "Upload", desc: "Twitter Fleets", src: "../assets/images/logo-twitter.png" },
                        { title: "Sync", desc: "LinkedIn Stories", src: "../assets/images/logo-linkedin.png" }
                    ],
                        businessBenefits: [
                            { header: "Brands", icon: "../assets/icons/ic-stock-black.svg", title: "Icreases conversions", desc: "Enhance the visual showcase of your products with unique content making visitors convert to buyers." },
